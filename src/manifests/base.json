{
  "name": "Material Icons for Web",
  "version": "1.8.15",
  "description": "Material icons for the file browser of popular websites like GitHub, Azure, Bitbucket etc.",
  "homepage_url": "https://github.com/material-extensions/material-icons-browser-addon",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://bitbucket.org/*",
        "*://dev.azure.com/*",
        "*://*.visualstudio.com/*",
        "*://gitea.com/*",
        "*://gitlab.com/*",
        "*://gitee.com/*",
        "*://sourceforge.net/*"
      ],
      "js": ["./main.js"],
      "css": ["./injected-styles.css"],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": ["storage", "activeTab", "scripting"]
}
